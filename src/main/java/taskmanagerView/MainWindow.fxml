<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.paint.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="800.0" prefWidth="1050.0" style="-fx-background-color: #3D5B5D;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="taskmanagerView.MainWindowController">
   <children>
      <VBox layoutY="80.0" prefHeight="722.0" prefWidth="163.0" style="-fx-background-color: white;">
         <children>
            <Button fx:id="addTask" mnemonicParsing="false" onAction="#addNewTask" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Create New Task" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" top="3.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="editTask" mnemonicParsing="false" onAction="#editTask" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Change Task" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="deleteTask" mnemonicParsing="false" onAction="#deleteTask" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Delete Task" textFill="WHITE">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="editCategories" mnemonicParsing="false" onAction="#editCategories" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Categories" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="editContributors" mnemonicParsing="false" onAction="#editContributors" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Contributors" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Chart" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" onAction="#saveToXML" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Export to XML" textFill="WHITE" fx:id="xmlExport">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="CSV" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button mnemonicParsing="false" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Dropbox" textFill="WHITE">
               <VBox.margin>
                  <Insets bottom="5.0" />
               </VBox.margin>
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="updateTask" mnemonicParsing="false" onAction="#handleTasks" prefHeight="36.0" prefWidth="166.0" style="-fx-background-color: #517E81;" text="Update" textFill="WHITE">
               <font>
                  <Font size="15.0" />
               </font></Button>
         </children>
      </VBox>
      <Label layoutX="14.0" layoutY="27.0" prefHeight="35.0" prefWidth="291.0" text="Taskmanagement" textFill="#fffdfd">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TableView fx:id="taskView" layoutX="175.0" layoutY="80.0" prefHeight="722.0" prefWidth="864.0">
        <columns>
            <TableColumn fx:id="descriptionColumn" prefWidth="129.0" text="Description" />
            <TableColumn fx:id="detailedDescriptionColumn" prefWidth="145.0" text="Detail description" />
            <TableColumn fx:id="dueDateColumn" prefWidth="125.0" sortType="DESCENDING" text="Due date" />
            <TableColumn fx:id="contributorsColumn" prefWidth="121.0" text="Contributors" />
            <TableColumn fx:id="categoriesColumn" prefWidth="130.0" text="Categories" />
            <TableColumn fx:id="attachmentsColumn" prefWidth="130.0" text="Attachments" />
            <TableColumn fx:id="doneCheckBoxColumn" prefWidth="83.0" text="Done?" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
