            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="deleteCategoryButton" layoutX="679.0" layoutY="737.0" mnemonicParsing="false" onAction="#deleteCategory" text="Delete Category">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
